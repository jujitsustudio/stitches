{
  "name": "@jujst/stitches",
  "version": "0.1.4",
  "bugs": {
    "url": "https://github.com/jujitsustudio/stitches/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jujitsustudio/stitches.git",
    "directory": "packages/stitches"
  },
  "license": "MIT",
  "author": "KIJVCN <kijv999@gmail.com>",
  "sideEffects": false,
  "exports": {
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs"
    },
    "./postcss": {
      "types": "./dist/postcss.d.ts",
      "import": "./dist/postcss.mjs",
      "require": "./dist/postcss.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@swc/wasm": "^1.3.102",
    "jiti": "^1.21.0",
    "postcss": "^8.4.33",
    "postcss-discard-empty": "^6.0.1",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "tsconfig": "^7.0.0",
    "tsconfig-paths": "^4.2.0",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/safe-json-stringify": "^1.1.5",
    "fast-glob": "^3.3.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "safe-json-stringify": "^1.2.0",
    "ts-pattern": "^5.0.6"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
